<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_ffffff"
    >
    <include layout="@layout/menu_title_layout"/>
    <RelativeLayout
        android:id="@+id/card_stock_manage_batchCardStockLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_authImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/batch_secur_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="批量制劵"
            android:layout_toRightOf="@id/auth_manage_authImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />


</LinearLayout>
