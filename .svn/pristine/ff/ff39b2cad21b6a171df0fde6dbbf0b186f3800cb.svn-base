<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_f8f8f8"
     >
    
     <include layout="@layout/menu_title_layout"/>

	<RelativeLayout
		android:id="@+id/setting_transaction_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_transactionImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/transaction_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="交易设置"
			android:layout_toRightOf="@id/setting_transactionImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
	<RelativeLayout
		android:id="@+id/setting_print_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_printImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/print_setting_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="打印设置"
			android:layout_toRightOf="@id/setting_printImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
	<RelativeLayout
		android:id="@+id/setting_money_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_moneyImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/set_price_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="金额设置"
			android:layout_toRightOf="@id/setting_moneyImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
	<RelativeLayout
		android:id="@+id/setting_business_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_businessImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/bus_info_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="商户信息"
			android:layout_toRightOf="@id/setting_businessImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
	<RelativeLayout
		android:id="@+id/setting_clearWater_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_clearWaterImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/clear_water_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="清空流水"
			android:layout_toRightOf="@id/setting_clearWaterImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		android:id="@+id/antu_manage_view5"
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
	<RelativeLayout
		android:id="@+id/setting_version_layout"
		style="@style/manage_menu_relativelayout"
		>
		<ImageView
			android:id="@+id/setting_versionImg"
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/about_icon"
			android:layout_centerVertical="true"
			/>
		<TextView
			style="@style/manage_menu_textview"
			android:text="关于悦收银"
			android:layout_toRightOf="@id/setting_versionImg"
			/>
		<ImageView
			android:layout_width="20dp"
			android:layout_height="20dp"
			android:src="@drawable/next_icon"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			/>
	</RelativeLayout>
	<View
		android:id="@+id/antu_manage_view6"
		style="@style/divider_line1dp"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		/>
</LinearLayout>
