<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <include layout="@layout/search_title_layout"/>

    <View
        style="@style/divider_line1dp"
        ></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:background="@color/white_ffffff"
        android:gravity="center_vertical"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="卡劵名称"
            android:textSize="@dimen/TextSize16"
            android:textColor="@color/black_333"
            android:layout_weight="1"
            android:gravity="center_vertical"
            />
        <View
            style="@style/divider_line1dp_h"
            ></View>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="核销劵码"
            android:textSize="@dimen/TextSize16"
            android:textColor="@color/black_333"
            android:layout_weight="1"
            android:gravity="center"
            />
    </LinearLayout>
    <View
        style="@style/divider_line1dp"
        ></View>
    <com.wanding.xingpos.view.XListView
        android:id="@+id/check_card_stock_recode_xListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white_f2f2f2"
        android:divider="@color/white_f2f2f2"
        android:dividerHeight="1dp"
        />

</LinearLayout>
