<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_f2f2f2"
    >
    <include layout="@layout/menu_title_layout"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        >
        <TextView
            android:id="@+id/check_card_stock_tvTitle"
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:text="核销劵码"
            android:textSize="@dimen/TextSize16"
            android:textColor="@color/black_000000"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            />
        <ImageView
            android:id="@+id/check_card_stock_imgScan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/scan_open_icon"
            android:padding="15dp"
            android:layout_alignParentRight="true"
            />
        <EditText
            android:id="@+id/check_card_stock_etNum"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="请输入核销劵码！"
            android:text=""
            android:textSize="@dimen/TextSize14"
            android:textColor="@color/black_333"
            android:inputType="number"
            android:background="@null"
            android:layout_marginLeft="5dp"
            android:paddingLeft="5dp"
            android:layout_toLeftOf="@id/check_card_stock_imgScan"
            android:layout_toRightOf="@id/check_card_stock_tvTitle"
            />
        <!-- 20180413133134725640612990473024 -->
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/check_card_stock_tvOk"
            style="@style/button_styles"
            android:layout_marginTop="10dp"
            android:layout_margin="20dp"
            ></TextView>
    </LinearLayout>


</LinearLayout>
