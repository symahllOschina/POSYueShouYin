package com.wanding.xingpos.bean;

import java.io.Serializable;
import java.util.List;

/**
 * 结算，交接班记录返回实体
 */
public class ShiftRecordListResData implements Serializable {

	private Integer countRow;//": 7,
	private Integer numPerPage;//": 1,
	private Integer totalCount;//": 7,
	private Integer pageNum;//": 1
	

	private List<ShiftRecordResData> orderList;


	public ShiftRecordListResData() {
		super();
		// TODO Auto-generated constructor stub
	}


	public Integer getCountRow() {
		return countRow;
	}


	public void setCountRow(Integer countRow) {
		this.countRow = countRow;
	}


	public Integer getNumPerPage() {
		return numPerPage;
	}


	public void setNumPerPage(Integer numPerPage) {
		this.numPerPage = numPerPage;
	}


	public Integer getTotalCount() {
		return totalCount;
	}


	public void setTotalCount(Integer totalCount) {
		this.totalCount = totalCount;
	}


	public Integer getPageNum() {
		return pageNum;
	}


	public void setPageNum(Integer pageNum) {
		this.pageNum = pageNum;
	}


	public List<ShiftRecordResData> getOrderList() {
		return orderList;
	}


	public void setOrderList(List<ShiftRecordResData> orderList) {
		this.orderList = orderList;
	}
	
	
	
	
	
}
