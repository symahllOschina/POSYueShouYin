<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_ffffff"
    >
    <include layout="@layout/menu_title_layout"/>
    <RelativeLayout
        android:id="@+id/auth_manage_authLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_authImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_manage_auth_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="预授权"
            android:layout_toRightOf="@id/auth_manage_authImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_authConfirmLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_authConfirmImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_confirm_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="预授权完成"
            android:layout_toRightOf="@id/auth_manage_authConfirmImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_authCancelLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_authCancelImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_cancel_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="预授权撤销"
            android:layout_toRightOf="@id/auth_manage_authCancelImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_authConfirmCancelLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_authConfirmCancelImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_confirm_cancel_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="预授权完成撤销"
            android:layout_toRightOf="@id/auth_manage_authConfirmCancelImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_scanAuthLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_scanAuthImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_manage_auth_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="扫码预授权"
            android:layout_toRightOf="@id/auth_manage_scanAuthImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/antu_manage_view5"
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_scanAuthConfirmLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_scanAuthConfirmImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_confirm_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="扫码预授权完成"
            android:layout_toRightOf="@id/auth_manage_scanAuthConfirmImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/antu_manage_view6"
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/auth_manage_scanAuthCancelLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/auth_manage_scanAuthCancelImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/auth_cancel_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="扫码预授权撤销"
            android:layout_toRightOf="@id/auth_manage_scanAuthCancelImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/antu_manage_view7"
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />


</LinearLayout>
