package com.wanding.xingpos.bean;

import java.io.Serializable;
import java.util.ArrayList;

/**  结算返回实体   */
public class ShiftResData implements Serializable {
	
	private ArrayList<SubTimeSummaryResData> timeList;
	private ArrayList<SubReocrdSummaryResData> reocrdList;
	private ArrayList<SubTotalSummaryResData> totalList;
	public ShiftResData() {
		super();
		// TODO Auto-generated constructor stub
	}

	public ArrayList<SubTimeSummaryResData> getTimeList() {
		return timeList;
	}
	public void setTimeList(ArrayList<SubTimeSummaryResData> timeList) {
		this.timeList = timeList;
	}
	public ArrayList<SubReocrdSummaryResData> getReocrdList() {
		return reocrdList;
	}
	public void setReocrdList(ArrayList<SubReocrdSummaryResData> reocrdList) {
		this.reocrdList = reocrdList;
	}
	public ArrayList<SubTotalSummaryResData> getTotalList() {
		return totalList;
	}
	public void setTotalList(ArrayList<SubTotalSummaryResData> totalList) {
		this.totalList = totalList;
	}
	
	
}
